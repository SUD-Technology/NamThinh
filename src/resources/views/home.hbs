<script>
    $(document).ready(function() {
        $('.owl-carousel').owlCarousel({
            loop: true,
            margin: 10,
            nav: true,
            autoplay: true,
            responsive: {
                0: {
                    items: 3
                },
                600: {
                    items: 5
                },
                1000: {
                    items: 8
                }
            }

            
        })

        const observer = new IntersectionObserver(entries => {
            entries.forEach(entry => {
                if(entry.isIntersecting) {
                    
                    
                    document.querySelectorAll(".classify-products")[0].classList.add("fadeLeft");
                    document.querySelectorAll(".classify-products")[1].classList.add("fadeRight");
                }
            })
        })
        const observer2 = new IntersectionObserver(entries => {
            entries.forEach(entry => {
                if(entry.isIntersecting) {
                    
                    
                    document.querySelectorAll(".classify-products")[2].classList.add("fadeLeft");
                    document.querySelectorAll(".classify-products")[3].classList.add("fadeRight");
                }
            })
        })

        observer.observe(document.querySelector("#tire-products"))
        observer2.observe(document.querySelector("#tool-products"))
    })
</script>

